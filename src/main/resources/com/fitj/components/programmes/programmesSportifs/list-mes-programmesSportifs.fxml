        <Text fx:id="errorText" fill="RED" layoutX="129.0" layoutY="455.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Error Message" textAlignment="CENTER" wrappingWidth="642.0" />
        <Button fx:id="detailProgrammeNutritionButton" layoutX="207.0" layoutY="464.0" mnemonicParsing="false" onAction="#goToDetailProgrammeNutrition" styleClass="buttonBlue" text="Voir" />
    </children>
</Pane>
