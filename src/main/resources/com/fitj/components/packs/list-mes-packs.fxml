<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="600.0" prefWidth="900.0" scaleShape="false" stylesheets="@../../styles/style.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fitj.controllers.packs.ControllerMesPacksList">
    <children>
        <Button fx:id="addPackButton" layoutX="455.0" layoutY="519.0" mnemonicParsing="false" onAction="#goToAddPack" prefHeight="26.0" prefWidth="116.0" style="-fx-background-color: #054371;" styleClass="buttonBlue" text="Ajouter" textFill="WHITE" />
        <ListView fx:id="listView" layoutX="17.0" layoutY="25.0" onMouseClicked="#selectItemPack" prefHeight="450.0" prefWidth="867.0" styleClass="emptyRectangle" />
        <Button fx:id="updatePackButton" layoutX="320.0" layoutY="519.0" mnemonicParsing="false" onAction="#goToUpdatePack" prefHeight="26.0" prefWidth="116.0" style="-fx-background-color: #054371;" styleClass="buttonBlue" text="Modifier" textFill="WHITE" />
        <Button fx:id="deletePackButton" layoutX="589.0" layoutY="519.0" mnemonicParsing="false" onAction="#deletePack" prefHeight="26.0" prefWidth="116.0" style="-fx-background-color: #054371;" styleClass="buttonBlue" text="Supprimer" textFill="WHITE" />
        <Text fx:id="errorText" fill="RED" layoutX="128.0" layoutY="501.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Error Message" textAlignment="CENTER" wrappingWidth="642.0" />
        <Button fx:id="detailPackButton" layoutX="205.0" layoutY="519.0" mnemonicParsing="false" onAction="#goToDetailPack" styleClass="buttonBlue" text="Voir" />
    </children>
</Pane>

