       <Button fx:id="deleteMaterielbutton" layoutX="528.0" layoutY="531.0" mnemonicParsing="false" onAction="#deleteMateriel" prefHeight="26.0" prefWidth="116.0" style="-fx-background-color: #054371;" styleClass="buttonBlue" text="Supprimer" textFill="WHITE" />
      <Text fx:id="errorText" fill="RED" layoutX="130.0" layoutY="520.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Error Message" textAlignment="CENTER" wrappingWidth="642.0" />
   </children>
</Pane>
